{
  "name": "aeternum-stock",
  "version": "1.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "automatic",
  "homepage": "/inventory/",
  "splash": {
    "image": "./assets/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#343a40"
  },
  "scripts": {
    "start": "expo start",
    "start:prod": "serve -s dist -l 8080 --ssl-cert C:/Aeternum-Stock/backend/ssl/certificate.cer --ssl-key C:/Aeternum-Stock/backend/ssl/private.key",
    "android": "expo start --android",
    "android:build": "expo build:android",
    "ios": "expo start --ios",
    "ios:build": "expo build:ios",
    "web": "expo start --web",
    "web:build": "expo export --platform web && cp web/index.html dist/index.html"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "expo": "^51.0.8",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.14",
    "expo-image": "~1.12.12",
    "expo-sqlite": "~14.0.3",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.6",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-crypto-js": "^1.0.0",
    "react-native-date-picker": "^5.0.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.30.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
